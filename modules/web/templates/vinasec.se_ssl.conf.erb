<VirtualHost *:443>
    SSLEngine on
    SSLCertificateFile /etc/letsencrypt/live/greyhash.se/cert.pem
    SSLCertificateKeyFile /etc/letsencrypt/live/greyhash.se/privkey.pem
    SSLCertificateChainFile /etc/letsencrypt/live/greyhash.se/fullchain.pem

   # Website name and alias
  ServerName  greyhash.se
  ServerAlias www.greyline.se greyline.se www.vinasec.se vinasec.se 

  Redirect permanent / https://www.greyhash.se/

</VirtualHost>
